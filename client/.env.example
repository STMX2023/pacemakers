# Supabase configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Redirect URI configuration
APP_REDIRECT_URI=pacemakers://auth/callback
EXPO_REDIRECT_SCHEME=pacemakers

# Additional scheme configurations
IOS_BUNDLE_IDENTIFIER=com.x779wv3.pacemakers
ANDROID_PACKAGE=com.x779wv3.pacemakers
EXPO_DEV_REDIRECT=exp://localhost:19000/--/auth/callback

# App configuration
APP_NAME=client
APP_SLUG=client
APP_VERSION=1.0.0
APP_SCHEME=com.x779wv3.pacemakers
APP_ORIENTATION=portrait
APP_ICON=./assets/images/icon.png
APP_USER_INTERFACE_STYLE=automatic

# Android configuration
ANDROID_FOREGROUND_IMAGE=./assets/images/adaptive-icon.png
ANDROID_BACKGROUND_COLOR=#ffffff

# iOS configuration
IOS_SUPPORTS_TABLET=true

# Web configuration
WEB_BUNDLER=metro
WEB_OUTPUT=static
WEB_FAVICON=./assets/images/favicon.png

# Splash screen configuration
SPLASH_IMAGE=./assets/images/splash-icon.png
SPLASH_IMAGE_WIDTH=200
SPLASH_RESIZE_MODE=contain
SPLASH_BACKGROUND_COLOR=#ffffff

# Default fallback values
DEFAULT_APP_NAME=client
DEFAULT_APP_SLUG=client
DEFAULT_APP_VERSION=1.0.0
DEFAULT_APP_SCHEME=com.x779wv3.pacemakers
DEFAULT_IOS_BUNDLE_ID=com.x779wv3.pacemakers
DEFAULT_ANDROID_PACKAGE=com.x779wv3.pacemakers 